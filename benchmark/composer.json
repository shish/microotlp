{
    "require": {
        "slim/slim": "^4",
        "slim/psr7": "^1",
        "open-telemetry/sdk": "^1.8",
        "open-telemetry/opentelemetry-logger-monolog": "^1.1",
        "open-telemetry/exporter-otlp": "^1.3",
        "php-http/guzzle7-adapter": "^1.1",
        "shish/microotlp": "dev-main"
    },
    "minimum-stability": "dev",
    "repositories": [
      {
        "type": "path",
        "url": "..",
        "options": {
          "symlink": true
        }
      }
    ],
    "config": {
        "allow-plugins": {
            "php-http/discovery": false,
            "tbachert/spi": false
        }
    }
}
